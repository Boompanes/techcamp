<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.findeds.traveltracker.MainActivity"
        tools:ignore="MergeRootFrame">

    <RelativeLayout
            android:id="@+id/container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

    </RelativeLayout>

    <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

        <com.slidinglayer.SlidingLayer
                android:id="@+id/slidingLayer1"
                android:layout_width="@dimen/layer_width"
                android:layout_height="match_parent">
            <!--slidingLayer:shadowDrawable="@drawable/sidebar_shadow"
                slidingLayer:shadowWidth="@dimen/shadow_width"
                slidingLayer:offsetWidth="@dimen/offset_width"
                slidingLayer:stickTo="auto|right|left|middle"
                slidingLayer:closeOnTapEnabled="true">-->

            <include
                    android:id="@+id/travel_list"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    layout="@layout/travel_list"/>

        </com.slidinglayer.SlidingLayer>
    </RelativeLayout>

</FrameLayout>